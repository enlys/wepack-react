{
  "name": "tsExpressDemoWebServer",
  "version": "1.0.0",
  "path": "/demo/",
  "main": "dist/index.js",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9",
    "iOS >= 8",
    "Android >= 4"
  ],
  "scripts": {
    "dev": "concurrently -k \"npm run dev:client\" \"npm run dev:server\"",
    "build": "npm-run-all clean --parallel build:*",
    "clean": "rimraf public/assets dist release",
    "start": "node dist/index.js",
    "dev:client": "webpack-dev-server --config config/webpack.dev.js",
    "dev:server": "nodemon --exec ts-node -w src src/index.ts",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:server": "tsc && cp -r src/config dist/config && cp -r src/views dist/views",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json && git add .",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "release": "npm-run-all clean build release:copy release:deps release:package",
    "release:copy": "cross-env-shell \"shx mkdir -p release/$npm_package_name && shx cp -r dist/ public/ *.json release/$npm_package_name\" ",
    "release:deps": "cross-env-shell \"cd release/$npm_package_name && npm install --production\" ",
    "release:package": "cross-env-shell \"cd release && tar -zcvf $npm_package_name.tar.gz $npm_package_name\" "
  },
  "dependencies": {
    "@up/taf-conf": "^1.3.0",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-proxy-middleware": "^2.0.1",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-promise": "^0.5.29",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "antd-mobile": "^5.25.1",
    "assets-webpack-plugin": "^7.1.1",
    "autoprefixer": "^10.3.7",
    "axios": "^0.23.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "cssnano": "^5.0.8",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.4.0",
    "immer": "^9.0.16",
    "less": "^4.1.2",
    "less-loader": "^10.1.0",
    "mini-css-extract-plugin": "^2.4.2",
    "nodemon": "^2.0.15",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.2.0",
    "postcss-px-to-viewport": "^1.1.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "redux-persist": "^6.0.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.2",
    "rimraf": "^3.0.2",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  }
}
